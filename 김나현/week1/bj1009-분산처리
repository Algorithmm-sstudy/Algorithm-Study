#include <iostream>
using namespace std;

int main() {
	int T = 0;

	cin >> T;

	for (int i = 0; i < T; i++) {
		int a;
		int b;
		int data = 1;

		cin >> a;
		cin >> b;

		for (int k = 0; k < b; k++) {
			data = (data * a) % 10;
		}


		if (data % 10 == 0)
			cout << 10;
		else
			cout << data % 10;
		cout << endl;
	}
}
