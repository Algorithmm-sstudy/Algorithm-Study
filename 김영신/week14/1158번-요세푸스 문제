#include <iostream>
#include <queue>

using namespace std;

queue<int> q;

int main() {
    int n, k;
    cin >> n >> k;

    for (int i = 0; i< n; i++) {
        q.push(i+1);
    }
    int kIndex;
    cout << "<";
    while (!q.empty()) {
        for (int i = 0; i < k-1; i++) {
            kIndex = q.front();
            q.pop();
            q.push(kIndex);
        }
        
        cout << q.front();
        if (q.size() != 1) cout << ", ";
        q.pop();
    }
    cout << ">";
}

