#include <iostream>
using namespace std;

int main() {
    int stick = 64;
    int x;
    cin >> x;

    int min = 64;
    int cnt = 1;
    while (1) {
        if (stick == x) {
            cout << cnt;
            break;
        }
        min /= 2;
        if (stick - min >= x) { // 절반 중 하나를 버림
            stick -= min;
        }
        else {
            cnt++;
        }
    }
}
