#include <iostream>
using namespace std;

int main() {
	// A: 한 시간 일하면 쌓이는 피로도, B: 한 시간에 처리할 수 있는 일
	// C: 한 시간 쉬면 줄어드는 피로도, M: 피로도의 최대치.
	int A, B, C, M;
	int fatigue = 0; // 피로도
	int work = 0; //처리한 일의 양
	
	cin >> A >> B >> C >> M;

	for (int i = 0; i < 24; i++) {
		if ((fatigue + A) <= M) {
			fatigue += A;
			work += B;
		}
		else if ((fatigue + A) > M) {
			fatigue -= C;

			//피로도가 음수로 내려가면 0으로 바뀜.
			if (fatigue < 0) {
				fatigue = 0;
			}
		}
	}

	cout << work << endl;

	return 0;
}
