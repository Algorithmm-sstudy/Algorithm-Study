#include<iostream>
using namespace std;

int main() {
    int t;
    cin>>t;

    int v[12];

    int n;

    v[1]=1; //1을 나타낼 수 있는 경우의 수가 1로 1개
    v[2]=2; //2를 나타낼 수 있는 경우의 수가 1+1, 2로 2개
    v[3]=4; //3을 나타낼 수 있는 경우의 수가 1+1+1, 1+2, 2+1, 3으로 4개
  

    while (t--) {
        cin>>n;    //n입력받기

        for (int i=4;i<=n;i++) {
            v[i]=v[i-1]+v[i-2]+v[i-3]; //앞에 있는 3항을 더하면 나온다
        }

        cout<<v[n]<<"\n";
    }
}
